{{render 'assignee' this}}
<ul class='filters'>
  <h5>Linked repos</h5>
  {{#each filter in linkFilters }}
    {{view App.FilterView lastClicked=lastLinkFilterClicked name=filter.name mode=filter.mode color=filter.color}}
  {{/each}}

  <h5>Assignment</h5>
  {{#each filter in userFilters }}
    {{view App.FilterView lastClicked=lastUserFilterClicked name=filter.name mode=filter.mode color=filter.color }}
  {{/each}}

  <h5>Milestones</h5>
  {{#each filter in milestoneFilters }}
    {{view App.FilterView lastClicked=lastMilestoneFilterClicked name=filter.name mode=filter.mode color=filter.color }}
  {{/each}}
  <h5>Labels</h5>
  {{#each filter in labelFilters }}
    {{view App.FilterView lastClicked=lastLabelFilterClicked name=filter.name mode=filter.mode color=filter.color tagType="filtered-label"}}
  {{/each}}

</ul>
