{{component headerClass column=model issues=sortedIssues isCollapsed=isCollapsed}}

{{#if isCreateVisible}}
  {{columns/hb-quick-issue target=target}}
{{/if}}

<ul class="cards sortable">
  {{#each sortedIssues as |issue|}}
    {{
      hb-card
        issue=issue
        issues=sortedIssues
        repos=repos
        templateName=cardClass
        target=target
    }}
  {{/each}}
</ul>
<div class="collapsed"></div>
