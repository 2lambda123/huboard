<div data-module="modules/workqueue/main" data-parameters='<%= @parameters.to_json %>' id="workqueue">
</div>

<% content_for :title do %><%=@parameters[:repo]%> :: <% end %>

<% content_for :header do %>
  <h1 class="header">
    <span class="author"><%= h @parameters[:user] %></span>
    /
    <span class="repo"><%= h @parameters[:repo] %></span>
  </h1>

<% end %>

<% content_for :navigation do %>

  <li class="active">
  <a href="/<%=@parameters[:user]%>/<%=@parameters[:repo]%>/milestones">Milestones</a>
  </li>
  <li>
  <a href="/<%=@parameters[:user]%>/<%=@parameters[:repo]%>/board">Tasks</a>
  </li>

<% end %>
